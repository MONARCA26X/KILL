local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/iblameaabis/Ililililililililililililil/refs/heads/main/W%20UI", true))()

local player = game.Players.LocalPlayer
local displayName = player.DisplayName or player.Name

local window = library:AddWindow("Private KILL MON4RK | Hello " .. displayName, {
    main_color = Color3.fromRGB(255, 255, 255),
    min_size = Vector2.new(400, 870),
    can_resize = false,
})
